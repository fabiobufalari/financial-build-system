# Environment Configuration for Financial Build System
# EN: Environment variables for production deployment
# PT: Variáveis de ambiente para deploy de produção

# API Base URL - New domain configuration
VITE_API_BASE_URL=https://buildingsystem.ddns.net:8443

# Demo mode configuration
VITE_DEMO_MODE=false

# Application configuration
VITE_APP_NAME=Financial Build System
VITE_APP_VERSION=2.0.0

# Logging configuration
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=info

# Multi-language support
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,pt,fr,zh,ar

